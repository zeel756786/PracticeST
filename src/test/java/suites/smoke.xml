            <class name = "web.tests.projectBuilder.projectBuilderTests" >
                <methods>
                    <include name="verify_when_click_on_project_builder"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "verify_participant_bulk_on_boarding_scenario">
        <classes>
            <class name = "web.tests.participants.participantsTests" >
                <methods>
                    <include name="verify_participant_bulk_on_boarding_scenario"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "verify_participant_single_on_boarding_scenario">
        <classes>
            <class name = "web.tests.participants.participantsTests" >
                <methods>
                    <include name="verify_participant_single_on_boarding_scenario"></include>
                </methods>
            </class>
        </classes>
