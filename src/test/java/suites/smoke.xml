<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "Smoke ">
    <test name = "Verify_login_with_valid_credentials">
        <classes>
            <class name = "web.tests.login.loginTests" >
                <methods>
                    <include name="Verify_login_with_valid_credentials"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "Verify_login_with_invalid_credentials">
        <classes>
            <class name = "web.tests.login.loginTests" >
                <methods>
                    <include name="Verify_login_with_invalid_credentials"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "verify_when_click_on_project_builder">
